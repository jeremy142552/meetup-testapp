<div class="container">
  <h1>Upload CSV File</h1>

  <%= form_with url: import_users_groups_path, method: :post, multipart: true do |form| %>
    <div class="form-group">
      <%= form.label :file, 'CSV File' %>
        <%= form.file_field :file, class: 'form-control-file' %>
    </div>
    <div class="form-group">
      <%= form.submit 'Import Users' , class: 'btn btn-primary' %>
    </div>
    <% end %>

      <%= link_to 'Back to Groups' , groups_path, class: 'btn btn-secondary' %>
</div>